public class Shippingcost {

	public String sendername1,recievername1,sendername2,recievername2;
	public int weight1,weight2;

	
	public int shippingcost1(int cpp1)
	{
		return cpp1*weight1;
	}
	
	
	public int shippingcost2(int cpp2)
	{
		return cpp2*weight2;
	}
	
	
}
